<% layout('/layouts/boilerplate') %>

    <body>
        <h3>All Listings</h3>
        <br>
        <form action="/listings/new" method="get">
            <button>Create new listings</button>
        </form>
        <ul>
            <% for(let listing of allListing ) { %>
                <li><a href="/listings/<%= listing._id%>">
                        <%=listing.title%>
                    </a></li>
                <% } %>
        </ul>
    </body>